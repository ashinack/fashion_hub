<section>
    <div class="container mt-5 col-md-12">






        <table class='table display' id="User_Table">
            <thead>
                <tr>

                    <th scope="col">Date</th>
                    <th scope="col">Address</th>
                    <th scope="col">Status</th>
                    <th scope="col">Payment</th>
                    <th scope="col">Mobile</th>
                    <th scope="col">Amount</th>
                    {{!-- <th scope="col">Products</th>
                    <th></th> --}}
                    <th scope="col">Order Status</th>
                    <th scope="col"></th>
                    







                </tr>
            </thead>
            <tbody>

                {{#each orderList}}


                <tr>

                    <td><span style="font-size:x-small">{{this.OrderPlacedAt}}</span></td>
                    <td><span style="font-size:x-small">{{this.address}},{{this.pincode}},{{this.Place}}</span></td>
                    <td><span style="font-size:x-small">{{this.orderstatus}}</span></td>
                    <td><span style="font-size:x-small">{{this.paymentMethod}}</span></td>
                    <td><span style="font-size:x-small">{{this.mobile_no}}</span></td>
                    <td><span style="font-size:x-small">{{this.totalAmount}}</span></td>
                    {{!-- {{#each this.productdt}}
                    <td><img style="width: 50px; height:50px" src="/images/{{this.allImages.[0].maini}}" alt=""><br>
                    </td>

                    {{/each}} --}}
                    {{!-- <td></td> --}}
                    <td><a href="/admin/manage-orderstatus/{{this._id}}" class="btn btn-color text-white"
                            style="font-size:x-small">
                            shipped
                        </a></span></td>
                    <td><a href="/admin/manage-orderstatusdelivered/{{this._id}}" class="btn btn-color text-white"
                            style="font-size:x-small">
                            delivered
                        </a></td>
                        

                    {{/each}}






                </tr>
            </tbody>
        </table>

    </div>
</section>